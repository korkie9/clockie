{
  "expo": {
    "name": "Clockie",
    "slug": "clockie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/clockie-logo.png",
    "splash": {
      "image": "./assets/clockie-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/clockie-logo.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/clockie-logo.png"
    },
    "androidNavigationBar": {
      "visible": "immersive"
    }
  }
}
